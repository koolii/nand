 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=ele1);
    Nand(a=b, b=b, out=ele2);
    And(a=ele1, b=ele2, out=ele3);
    Not(in=ele3, out=out);
}
